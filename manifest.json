{
  "manifest_version": 2,

  "name": "UrlStack",
  "description": "Captures URL flow",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "48": "icon.png"
  },

  "permissions": ["tabs", "<all_urls>", "storage"],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
